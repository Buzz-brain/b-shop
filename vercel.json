{
    "version": 2,
    "builds": [
      {
        "use": "@vercel/static-build",
        "config": {
          "distDir": "public"
        }
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "index.ejs"
      },
      {
        "src": "/*",
        "dest": "index.js"
      }
    ]
  }